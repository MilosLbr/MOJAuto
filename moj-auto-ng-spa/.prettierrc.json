{
  "tabWidth": 4,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "bracketSpacing": true,
  "semi": true,
  "endOfLine": "crlf",
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "strict",
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
